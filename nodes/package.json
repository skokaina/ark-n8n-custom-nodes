{
  "name": "n8n-nodes-ark",
  "version": "1.0.0",
  "description": "n8n community nodes for ARK (Agentic Runtime for Kubernetes)",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "ark",
    "agents",
    "kubernetes",
    "ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/skokaina/ark-n8n-custom-nodes",
  "author": {
    "name": "Salah Kokaina",
    "url": "https://github.com/skokaina"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skokaina/ark-n8n-custom-nodes.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint credentials nodes --ext .ts",
    "lintfix": "eslint credentials nodes --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ArkApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ArkAgent/ArkAgent.node.js",
      "dist/nodes/ArkAgentAdvanced/ArkAgentAdvanced.node.js",
      "dist/nodes/ArkModel/ArkModel.node.js",
      "dist/nodes/ArkModelSelector/ArkModelSelector.node.js",
      "dist/nodes/ArkMemory/ArkMemory.node.js",
      "dist/nodes/ArkTeam/ArkTeam.node.js",
      "dist/nodes/ArkEvaluation/ArkEvaluation.node.js",
      "dist/nodes/ArkTool/ArkTool.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.39",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
